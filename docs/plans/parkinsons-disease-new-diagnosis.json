  "differential": [
    {
      "diagnosis": "Essential tremor",
      "features": "Action/postural tremor (not rest), bilateral, head tremor, family history, alcohol-responsive",
      "tests": "Clinical; DaTscan normal in ET"
    },
    {
      "diagnosis": "Drug-induced parkinsonism",
      "features": "Medication history (antipsychotics, metoclopramide), symmetric, may resolve with drug withdrawal",
      "tests": "Medication review; DaTscan often normal"
    },
    {
      "diagnosis": "Vascular parkinsonism",
      "features": "Lower-body predominant, gait disorder, stepwise progression, vascular risk factors, MRI white matter changes",
      "tests": "MRI brain; poor levodopa response"
    },
    {
      "diagnosis": "Progressive supranuclear palsy (PSP)",
      "features": "Early falls (backward), vertical gaze palsy, axial rigidity > limb, poor levodopa response",
      "tests": "MRI (hummingbird sign), clinical"
    },
    {
      "diagnosis": "Multiple system atrophy (MSA)",
      "features": "Early autonomic failure, cerebellar signs (MSA-C), poor levodopa response",
      "tests": "MRI (hot cross bun sign), MIBG preserved in MSA"
    },
    {
      "diagnosis": "Corticobasal degeneration (CBD)",
      "features": "Asymmetric apraxia, alien limb, cortical sensory loss, dystonia",
      "tests": "MRI (asymmetric cortical atrophy), clinical"
    },
    {
      "diagnosis": "Dementia with Lewy bodies (DLB)",
      "features": "Dementia before or within 1 year of parkinsonism, visual hallucinations, fluctuations",
      "tests": "Clinical criteria; DaTscan abnormal"
    },
    {
      "diagnosis": "Normal pressure hydrocephalus (NPH)",
      "features": "Gait apraxia (magnetic gait), urinary incontinence, dementia; triad",
      "tests": "MRI (ventriculomegaly), LP with large volume tap"
    },
    {
      "diagnosis": "Wilson disease",
      "features": "Age <50, KF rings, hepatic/psychiatric symptoms, tremor may be wing-beating",
      "tests": "Ceruloplasmin, 24h urine copper, slit lamp exam"
    }
  ],
