        {
          "item": "CBC (CPT 85025)",
          "ED": "-",
          "HOSP": "ROUTINE",
          "OPD": "ROUTINE",
          "ICU": "-",
          "rationale": "General health, rule out anemia",
          "target": "Normal"
        },
        {
          "item": "CMP (CPT 80053)",
          "ED": "-",
          "HOSP": "ROUTINE",
          "OPD": "ROUTINE",
          "ICU": "-",
          "rationale": "Hepatic/renal function for medication dosing",
          "target": "Normal"
        },
        {
          "item": "TSH (CPT 84443)",
          "ED": "-",
          "HOSP": "ROUTINE",
          "OPD": "ROUTINE",
          "ICU": "-",
          "rationale": "Hypothyroidism can cause slowness/tremor",
          "target": "Normal"
        },
        {
          "item": "Vitamin B12 (CPT 82607)",
          "ED": "-",
          "HOSP": "ROUTINE",
          "OPD": "ROUTINE",
          "ICU": "-",
          "rationale": "Deficiency can cause parkinsonism",
          "target": ">400 pg/mL"
